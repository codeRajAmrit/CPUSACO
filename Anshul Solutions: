Question 4 : 

#include <stdio.h>
#include <math.h>
#define PI 3.141592653589793

int main() {
    int income; 
    printf("Income : "); 
    scanf("%d", &income); 
    float tax; 

    if (income <= 100000) tax = 0; 
    else if (income <= 150000) tax = 0.10 * (income - 100000); 
    else tax = 5000 + 0.20 * (income - 150000); 
    
    printf("Income : %d, Tax : %.2f\n", income, tax); 
    
    return 0 ;
}

