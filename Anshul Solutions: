Question 2 : 

#include <stdio.h>

int main() {
    int n; 
    printf("Number of Students: "); 
    scanf("%d", &n); 

    int details[n][2]; 
    for (int i = 0; i < n; i += 1) {    
        printf("Roll Number and Marks: "); 
        scanf("%d %d", &details[i][0], &details[i][1]); 
    }

    // calculating average
    int total_Marks = 0; 
    for (int i = 0; i < n; i += 1) total_Marks += details[i][1]; 
    float average = (float) total_Marks / n; 

    // printing roll Numbers with marks less than average
    printf("Roll numbers with marks less than average: ");
    for (int i = 0; i < n; i += 1) {
        if (details[i][1] < average) printf("%d, ", details[i][0]); 
    }

    //finding roll numbers with maximum and minimum marks 
    int lowestIndex = 0, highestIndex = 0; 
    for (int i = 0; i < n; i += 1) {
        if (details[i][1] > details[highestIndex][1]) highestIndex = i; 
        if (details[i][1] < details[lowestIndex][1]) lowestIndex = i; 
    }

    printf("\nRoll No. with lowest marks: %d", details[lowestIndex][0]); 
    printf("\nRoll No. with highest marks: %d", details[highestIndex][0]); 

    return 0; 
}
